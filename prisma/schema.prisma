
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  
}

model User {
  id Int @id @default(autoincrement())
  phone String @unique
  email String @unique
  password String
  firstName String
  lastName String
}


model Otp {
  id        String   @id @default(uuid())
  email     String
  otp       String
  createdAt DateTime @default(now())
}